{
    "name": "Neo4j",
    "version": "0.1.21",
    "versionDate": "2020-10-30",
    "author": "hackolade",
    "engines": {
        "hackolade": "2.0.x",
        "hackoladePlugin": "1.0.0"
    },
    "contributes": {
        "target": {
            "applicationTarget": "NEO4J",
            "title": "Neo4j",
            "versions": [
                "3.x",
                "4.x"
            ]
        },
        "features": {
            "jaySchemaHasRequiredPatternField": false,
            "nestedCollections": {
                "level": 1,
                "isVirtual":true
            },
            "graphDB": true,
            "relationships": {
                "connectOnEntityLevel": true,
                "hasData": true
            },
            "enableForwardEngineering": {
                "jsonDocument": true,
                "jsonSchema": true,
                "excel": true,
                "plugin": true
            },
            "enableReverseEngineering": {
                "jsonDocument": true,
                "jsonSchema": true,
                "ddl": true,
                "xsd": true,
                "excel": true,
                "plugin": true
            },
            "FEScriptCommentsSupported": true
        }
    },
    "description": "Hackolade plugin for Neo4j",
    "disabled": false
}

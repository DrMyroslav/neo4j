{
    "name": "Neo4j",
    "version": "0.1.20",
    "versionDate": "2020-08-10",
    "author": "hackolade",
    "engines": {
        "hackolade": "2.0.x",
        "hackoladePlugin": "1.0.0"
    },
    "contributes": {
        "target": {
            "applicationTarget": "NEO4J",
            "title": "Neo4j",
			"versions": [
                "3.x",
                "4.x"
			]
		},
        "features": {
			"apiTarget": true,
            "jaySchemaHasRequiredPatternField": false,
            "nestedCollections": {
				"level": 1
            },
            "disableChildren": {
				"entityLevel": true
			},
            "graphDB": true,
            "relationships": {
                "connectOnEntityLevel": true,
                "hasData": true
            },
            "enableForwardEngineering": {
				"jsonDocument": true,
				"jsonSchema": true,
				"excel": true,
				"plugin": true
			},
			"enableReverseEngineering": {
				"jsonDocument": true,
				"jsonSchema": true,
				"ddl": true,
				"xsd": true,
				"excel": true,
				"plugin": true 
			}
        }
    },
    "description": "Hackolade plugin for Neo4j",
    "disabled": false
}
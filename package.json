{
    "name": "Neo4j",
    "version": "0.1.12",
    "versionDate": "2019-08-20",
    "author": "hackolade",
    "engines": {
        "hackolade": "2.0.x",
        "hackoladePlugin": "1.0.0"
    },
    "contributes": {
        "target": {
            "applicationTarget": "NEO4J",
            "title": "Neo4j",
			"versions": [
				"2.x",
				"3.x"
			]
		},
        "features": {
            "jaySchemaHasRequiredPatternField": false,
            "nestedCollections": false,
            "graphDB": true,
            "relationships": {
                "connectOnEntityLevel": true,
                "hasData": true
            },
            "enableForwardEngineering": {
				"jsonDocument": false,
				"jsonSchema": false,
				"excel": false,
				"plugin": true
			},
			"enableReverseEngineering": {
				"jsonDocument": false,
				"jsonSchema": false,
				"ddl": true,
				"xsd": true,
				"excel": false,
				"plugin": true 
			}
        }
    },
    "description": "Hackolade plugin for Neo4j",
    "disabled": false
}